<h1>Nuevo multimedia</h1>
<%= error_messages_for :multimedia %>
<% form_for(@multimedia, :url => admin_multimedias_path, :html => {:multipart => true}) do |f| %>
  <p>
    <b>Titulo</b><br />
    <%= f.text_field :titulo %>
  </p>

  <p>
    <b>Archivo</b><br />
    <%= f.upload_column_field :archivo %>
  </p>

  <p>
    <%= f.submit "Crear" %>
  </p>
<% end %>

<%= link_to 'Ir al listado', admin_multimedias_path %>
