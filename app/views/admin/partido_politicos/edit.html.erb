<h1>Editing partido_politico</h1>
<%= error_messages_for :partido_politico %>
<% form_for(@partido_politico, :url => admin_partido_politico_path) do |f| %>
  <p>
    <b>Partido politico</b><br />
    <%= f.text_field :partidoPolitico %>
  </p>
  <p>
    <b>Sigla</b><br />
    <%= f.text_field :sigla %>
  </p>
  <p>
    <%= f.submit "Actualizar" %>
  </p>
<% end %>
<%= link_to 'Mostrar', admin_partido_politico_path(@partido_politico) %> |
<%= link_to 'Ir al listado', admin_partido_politicos_path %>
