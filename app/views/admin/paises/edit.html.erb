<h1>Editar pais</h1>

<%= error_messages_for :paise %>

<% form_for(@paise, :url  => admin_paises_path ) do |f| %>
  <p>
    <b>Nombre</b><br />
    <%= f.text_field :pais %>
  </p>

  <p>
    <%= f.submit "Actualizar" %>
  </p>
<% end %>

<%= link_to 'Mostrar', @paise %> |
<%= link_to 'Ir al listado', admin_paises_path %>
