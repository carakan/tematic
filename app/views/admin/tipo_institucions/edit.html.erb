<h1>Editing tipo_institucion</h1>

<%= error_messages_for :tipo_institucion %>

<% form_for(@tipo_institucion, :url => admin_tipo_institucion_path) do |f| %>
  <p>
    <b>Tipo de institucion</b><br />
    <%= f.text_field :tipoInstitucion %>
  </p>

  <p>
    <%= f.submit "Actualizar" %>
  </p>
<% end %>

<%= link_to 'Mostrar', admin_tipo_institucion_path(@tipo_institucion) %> |
<%= link_to 'Ir al listado', admin_tipo_institucions_path %>
